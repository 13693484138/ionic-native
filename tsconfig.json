{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "declaration": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": true,
    "module": "es2015",
    "moduleResolution": "node",
    "paths": {
      "@ionic-native/core": ["./src/@ionic-native/core"]
    },
    "rootDir": "./src",
    "target": "es5",
    "skipLibCheck": true,
    "lib": ["es2015", "dom"]
  },
  "exclude": [
    "src/**/*.spec.ts",
    "node_modules",
    "dist"
  ]
}
